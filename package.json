{
  "name": "@caphub-funding/mainserver-provider",
  "version": "3.3.55",
  "description": "mainserver provider for react apps",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "scripts": {
    "prebuild": "npm run version:patch",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "update": "npx npm-check-updates -u",
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc && cp package.json ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CapHub-Funding/mainserver-provider.git"
  },
  "author": "Michael Nusair",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CapHub-Funding/mainserver-provider/issues"
  },
  "homepage": "https://github.com/CapHub-Funding/mainserver-provider#readme",
  "dependencies": {
    "mongoose": "^7.1.0",
    "typescript": "^5.0.4"
  }
}
